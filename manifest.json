{
  "name": "Hamburger",
  "description": "Bookmarker",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/",
    "bookmarks",
    "contextMenus"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Bookmarker",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
